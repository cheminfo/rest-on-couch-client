{
  "name": "rest-on-couch-client",
  "version": "1.0.0",
  "description": "A nodejs / browser client for rest-on-couch backend",
  "main": "./lib/index.js",
  "module": "./lib-es6/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib",
    "lib-es6"
  ],
  "scripts": {
    "clean": "rimraf lib lib-es6",
    "prepare": "npm run tsc",
    "test": "run-s test-only tslint",
    "test-only": "jest",
    "test-coverage": "jest --coverage",
    "tsc": "run-s clean tsc-es5 tsc-es6",
    "tsc-es5": "tsc",
    "tsc-es6": "tsc --project tsconfig.es6.json",
    "tslint": "tslint --project tsconfig.base.json",
    "tslint-fix": "npm run tslint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cheminfo/rest-on-couch-client.git"
  },
  "keywords": [],
  "author": "Daniel Kostro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cheminfo/rest-on-couch-client/issues"
  },
  "homepage": "https://github.com/cheminfo/rest-on-couch-client#readme",
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "devDependencies": {
    "@types/detect-node": "^2.0.0",
    "@types/jest": "^23.3.1",
    "@types/superagent": "^3.8.4",
    "babel-jest": "^23.6.0",
    "jest": "^23.6.0",
    "npm-run-all": "^4.1.2",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.1.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-cheminfo": "^1.0.2",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "immer": "^1.7.4"
  }
}
